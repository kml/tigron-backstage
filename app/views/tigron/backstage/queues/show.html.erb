<h1>Queue: <%= @queue.queue_name %></h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <% @queue.queue_settings.each do |(name, value)| %>
      <tr>
        <td><%= name %></td>
        <td><%= value %></td>
      </tr>
    <% end %>
  </tbody>
</table>


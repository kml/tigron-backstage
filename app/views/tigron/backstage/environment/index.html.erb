<h2>Services (MSC)</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>State</th>
      <%# <th>Substate</th> %>
      <th>Mode</th>
      <%# <th>Aliases</th> %>
      <%# <th>Parent</th> %>
      <th>Value Class</th>
      <th>Start Exception</th>
    </tr>
  </thead>
  <tbody>
    <% @msc_services.each do |service_controller| %>
      <tr>
        <td><%= service_controller.name %></td>
        <td><%= service_controller.state %></td>
        <%# <td><%= service_controller.substate %1></td> %>
        <td><%= service_controller.mode %></td>
        <%# <td><%= service_controller.aliases.to_a %1></td> %>
        <%# <td><%= service_controller.parent %1></td> %>
        <td><%= service_controller.value.class.name %></td>
        <td><%= service_controller.start_exception %></td>
      </tr>
    <% end %>
  </tbody>
</table>


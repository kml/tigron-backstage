<h2>Ruby Config</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <% @ruby.each do |(key, value)| %>
      <tr>
        <td><%= key %></td>
        <td><%= value %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h2>Gems</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Version</th>
      <th>Summary</th>
    </tr>
  </thead>
  <tbody>
    <% @gems.each do |(name, version, summary)| %>
      <tr>
        <td><%= name.html_safe %></td>
        <td><%= version %></td>
        <td><%= summary %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h2>Properties</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <% @properties.each do |(key, value)| %>
      <tr>
        <td><%= key %></td>
        <td><%= value %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h2>Input arguments</h2>

<%= @input_arguments %>

<h2>ENV</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <% @env.each do |(key, value)| %>
      <tr>
        <td><%= key %></td>
        <td><%= value %></td>
      </tr>
    <% end %>
  </tbody>
</table>

